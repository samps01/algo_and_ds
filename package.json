  "bugs": {
    "url": "https://github.com/samps01/algo_and_ds/issues"
  },
  "homepage": "https://github.com/samps01/algo_and_ds#readme",
  "dependencies": {
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^7.1.2",
